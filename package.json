{
  "name": "sveltekit-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "start": "node build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "fetch-schema": "curl https://raw.githubusercontent.com/Myphz/synq-be/refs/heads/master/src/server/protocol.ts -o src/api/protocol.ts",
    "generate-types": "npx -y supabase gen types typescript --project-id 'eicdozfiwnotmgsewwmj' --schema public > src/lib/types/supabase.ts",
    "android-debug": "bash android-debug.sh",
    "android-release": "bash android-release.sh"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.2.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.32.1",
    "svelte-check": "^4.2.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/device": "^7.0.1",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@capgo/capacitor-social-login": "^7.6.3",
    "@supabase/supabase-js": "^2.49.8",
    "core-js": "^3.42.0",
    "date-fns": "^4.1.0",
    "zod": "^3.25.28"
  }
}
