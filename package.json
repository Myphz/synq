{
  "name": "sveltekit-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "start": "node build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "fetch-schema": "curl https://raw.githubusercontent.com/Myphz/synq-be/refs/heads/master/src/server/protocol.ts -o src/lib/api/protocol.ts",
    "generate-types": "npx -y supabase gen types typescript --project-id 'eicdozfiwnotmgsewwmj' --schema public > src/lib/types/supabase.ts",
    "android-debug": "bash android-debug.sh",
    "android-release": "bash android-release.sh"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.45.8",
    "svelte-check": "^4.3.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/device": "^8.0.0",
    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/network": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capawesome/capacitor-file-picker": "^7.2.0",
    "@capgo/capacitor-social-login": "^7.20.0",
    "@supabase/supabase-js": "^2.87.1",
    "@svelte-put/clickoutside": "^4.0.0",
    "async-mutex": "^0.5.0",
    "core-js": "^3.47.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "linkify-html": "^4.3.2",
    "posthog-js": "^1.305.0",
    "zod": "^4.1.13"
  }
}
